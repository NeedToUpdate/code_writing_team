# Required environment variables for the MCP agent
# Copy this file to .env and fill in the values

# API keys and tokens
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GITHUB_PERSONAL_ACCESS_TOKEN=your_github_token_here
JIRA_TOKEN=your_jira_token_here
CONFLUENCE_TOKEN=your_confluence_token_here
GIT_AUTHOR_NAME=CodingTeam
GIT_AUTHOR_EMAIL=coding_team@email.com


# Optional: Specify a ticket ID (defaults to AI-7 if not set)
TICKET_ID=AI-7